<form action="<%= sessions_url %>" method="post">
  <input
    name="authenticity_token"
    type="hidden"
    value="<%= form_authenticity_token %>">

  <br>
  <label for="user_user_name">Username:</label>
  <input
    id="user_user_name"
    name="user[user_name]"
    type="text">

  <br>
  <label for="user_password">Password:</label>
  <input
    id="user_password"
    name="user[password]"
    type="password">

  <br>

  <input
    type="submit"
    value="Login">

</form>